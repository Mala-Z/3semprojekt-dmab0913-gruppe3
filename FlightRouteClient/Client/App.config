<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsHttpBinding>
                <binding name="wsHttpBinding_IFlightService" transactionFlow="true" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:9999/FlightService/" binding="wsHttpBinding"
                bindingConfiguration="wsHttpBinding_IFlightService" contract="FlightService.IFlightService"
                name="wsHttpBinding_IFlightService">
                <identity>
                    <userPrincipalName value="m-s-PC\m-s" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>