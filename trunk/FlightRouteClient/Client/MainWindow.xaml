<Window x:Name="Main" x:Class="Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:tabData="clr-namespace:Client.Tabs"
        Title="FlightRouteSystem" Height="600" Width="800" ResizeMode="NoResize" Icon="airplane.ico">
    <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="590" Margin="0,0,0,-18" VerticalAlignment="Top" Width="790" BorderThickness="1,1,0,1" SelectionChanged="TabControl_OnSelectionChanged">
        <TabItem x:Name="TabItemBooking" Header="Booking">
            <ContentControl x:Name="contentBooking"/>
        </TabItem>
        <TabItem x:Name="TabItemCustomer" Header="Kunder">
            <ContentControl x:Name="contentCustomer"/>
        </TabItem>
        <TabItem x:Name="TabItemFlight" Header="Flyforbindelser">
            <ContentControl x:Name="contentFlight"/>
        </TabItem>
        <TabItem x:Name="TabItemAirplane" Header="Fly">
            <ContentControl x:Name="contentAirplane"/>
        </TabItem>
        <TabItem x:Name="TabItemAirport" Header="Lufthavne">
            <ContentControl x:Name="contentAirport"/>
        </TabItem>
    </TabControl>
</Window>
