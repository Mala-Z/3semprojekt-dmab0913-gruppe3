      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300" Width="750" Height="100"
      Background="{DynamicResource {x:Static SystemColors.GradientInactiveCaptionBrushKey}}"
	Title="AddiPassenger">
    
    <Grid>
        <Label Content="Passager" HorizontalAlignment="Left" Margin="22,10,0,0" VerticalAlignment="Top"/>
