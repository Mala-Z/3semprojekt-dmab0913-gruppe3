        <TextBox x:Name="txtFornavn"  HorizontalAlignment="Left" Height="23" Margin="93,48,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="Efternavn" HorizontalAlignment="Left" Margin="254,45,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtEfternavn"  HorizontalAlignment="Left" Height="23" Margin="344,48,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="Email" HorizontalAlignment="Left" Margin="511,45,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtEmail" HorizontalAlignment="Left" Height="23" Margin="570,48,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Page>
